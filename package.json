{
  "name": "meow-share",
  "version": "1.0.0",
  "description": "P2P file sharing with WebRTC and server fallback",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "start": "node server/index.js",
    "build": "cd client && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["webrtc", "p2p", "file-sharing", "websocket"],
  "author": "",
  "license": "MIT"
}